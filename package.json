{
  "name": "RN_BoilerplateApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "androidDev": "react-native run-android --mode=devDebug --appIdSuffix dev",
    "androidDevRelease": "react-native run-android --mode=devRelease --appIdSuffix dev",
    "androidStaging": "react-native run-android --mode=stagingDebug --appIdSuffix staging",
    "androidStagingRelease": "react-native run-android --mode=stagingRelease --appIdSuffix staging",
    "androidProd": "react-native run-android --mode=prodDebug",
    "androidProdRelease": "react-native run-android --mode=prodRelease",
    "buildAndroidDevApk": "cd android && ./gradlew clean && ./gradlew createBundleDevReleaseJsAndAssets && ./gradlew assembleDevRelease && cd ..",
    "buildAndroidStagingApk": "cd android && ./gradlew clean && ./gradlew createBundleStagingReleaseJsAndAssets && ./gradlew assembleStagingRelease && cd ..",
    "buildAndroidProdApkAndBundle": "cd android && ./gradlew clean && ./gradlew createBundleProdReleaseJsAndAssets && ./gradlew assembleProdRelease && ./gradlew bundleProdRelease && cd ..",
    "android": "react-native run-android",
    "clean-android": "cd android && ./gradlew clean && cd ..",
    "ios": "react-native run-ios",
    "detox-build-ios": "detox build --configuration ios.sim.debug",
    "detox-test-ios": "detox test --configuration ios.sim.debug",
    "detox-build-android": "detox build --configuration android.emu.debug",
    "detox-test-android": "detox test --configuration android.emu.debug",
    "lint": "eslint .",
    "start": "react-native start",
    "linkAssets": "npx react-native-asset",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-vector-icons/ionicons": "12.3.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/native-stack": "^7.3.25",
    "@reduxjs/toolkit": "^2.8.2",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.15.3",
    "react-redux": "^9.2.0",
    "redux-saga": "^1.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "15.0.1",
    "@react-native-community/cli-platform-android": "15.0.1",
    "@react-native-community/cli-platform-ios": "15.0.1",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@react-native/metro-config": "0.81.0",
    "@react-native/typescript-config": "0.81.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^18.2.6",
    "@types/react-test-renderer": "^19.1.0",
    "babel-jest": "^29.6.3",
    "detox": "^20.40.2",
    "eslint": "^8.19.0",
    "eslint-plugin-testing-library": "^7.6.6",
    "jest": "^30.0.5",
    "msw": "^2.10.5",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "ts-jest": "^29.4.1",
    "typescript": "^5.6"
  },
  "engines": {
    "node": ">=20"
  }
}
